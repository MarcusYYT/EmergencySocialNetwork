{ 
  "plugins": ["plugins/markdown"], 
  "recurseDepth": 10, 
  "source": { 
    "include": ["./"], 
    "includePattern": ".mjs$", 
    "excludePattern": "node_modules"
  }, 
  "templates": { 
    "cleverLinks": true, 
    "monospaceLinks": true
  }, 
  "opts": { 
    "destination": "./jsdoc", 
    "recurse": true, 
    "readme": "./readme.md"
  } 
} 